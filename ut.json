  {"name": "PUID", "label": "PUID"},
  {"name": "PGID", "label": "PGID"},
  {"name": "VIRTUAL_HOST", "label": "Virtual Host for Proxy"},
  {"name": "VIRTUAL_PORT", "label": "Mapped Port for Proxy"},
  {"name": "HOSTNAME", "label": "Hostname"}
]
},
