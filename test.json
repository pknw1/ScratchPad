[
  {
  "title": "Nextcloud",
  "description": "Fileserver Interface",
  "logo": "https://cloudinovasi.id/assets/img/logos/caddy.png",
  "image": "nextcloud:latest",
  "platform": "linux",
  "ports": [
    "80/tcp"
  ],
  "restart_policy": "always",
  "categories": ["webserver", "file-sharing"],
  "privileged": true,
  "volumes": ["/host", "/var/www/html"],
   "note": "notes",
  "env": [
    {"name": "PUID", "type": "container", "label": "PUID", "set": "115"},
    {"name": "PGID", "label": "PGID", "set": "121"},
    {"name": "VIRTUAL_HOST", "label": "Virtual Host", "set": "owncloud1.pknw1.co.uk"},
    {"name": "VIRTUAL_PORT", "label": "Virtual Port", "set": "12380"},
    {"name": "HOSTNAME", "type": "container", "label": "Hostname", "set": "nextcloud.pknw1.co.uk"}
  ]
},
 "network": "bridged"
}
]
